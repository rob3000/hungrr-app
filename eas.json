{
  "cli": {
    "version": ">= 16.32.0",
    "appVersionSource": "remote"
  },
  "build": {
    "debug": {
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleDebug"
      },
      "developmentClient": true,
      "env": {
        "EXPO_PUBLIC_API_BASE_URL": "https://api-dev.hungrr.io/v1"
      }
    },
    "preview": {
      "android": {
        "buildType": "apk"
      },
      "env": {
        "EXPO_PUBLIC_API_BASE_URL": "https://api-dev.hungrr.io/v1"
      }
    },
    "production": {
      "env": {
        "EXPO_PUBLIC_API_BASE_URL": "https://api-dev.hungrr.io/v1"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
