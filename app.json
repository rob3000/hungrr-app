{
  "expo": {
    "name": "hungrr",
    "slug": "hungrr",
    "version": "1.0.0",
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "experiments": {
      "tsconfigPaths": true
    },
    "plugins": [
      [
        "react-native-bootsplash",
        {
          "assetsDir": "assets/bootsplash"
        }
      ],
      [
        "react-native-vision-camera",
        {
          "cameraPermissionText": "$(PRODUCT_NAME) needs access to your Camera to scan barcodes.",
          "enableCodeScanner": true
        }
      ]
    ],
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#f3eee5"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "$(PRODUCT_NAME) needs access to your Camera to scan barcodes."
      },
      "bundleIdentifier": "com.anonymous.hungrr"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#f3eee5"
      },
      "permissions": [
        "android.permission.CAMERA"
      ],
      "package": "com.anonymous.hungrr"
    },
    "extra": {
      "eas": {
        "projectId": "8b0df8a4-a095-4219-bf4a-9491e5d93f16"
      }
    }
  }
}
